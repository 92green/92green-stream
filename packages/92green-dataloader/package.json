{
  "version": "1.0.0-alpha.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "lint": "yarn eslint src/**/* --ext .js,.ts,.jsx,.tsx",
    "prepare": "tsdx build",
    "size": "yarn build && size-limit",
    "analyze": "size-limit --why",
    "test-all": "yarn test && yarn lint && yarn size"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "92green-dataloader",
  "author": "Damien Clarke",
  "module": "dist/green-dataloader.esm.js",
  "size-limit": [
    {
      "path": "dist/green-dataloader.cjs.production.min.js",
      "limit": "7 KB"
    },
    {
      "path": "dist/green-dataloader.esm.js",
      "limit": "7 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "eslint-config-blueflag": "^0.11.1",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "ts-jest": "^26.4.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dataloader": "2.0.0",
    "lru_map": "0.4.1"
  }
}
